{
    "name": "COPY COOKIES",
    "version": "1.0",
    "description": "copy cookie",
    "manifest_version": 2,
    "background": {
        "scripts": ["background.js"],
        "persistent": true
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": ["tabs", "notifications", "storage", "cookies", "http://*/*", "https://*/*" ]
}